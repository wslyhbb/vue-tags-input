{
  "name": "@wslyhbb/vue3-tags-input",
  "version": "3.0.6",
  "author": "Johannes Munari <johannesm@hotmail.de>",
  "contributors": [
    {
      "name": "Sinclair Chen",
      "url": "https://github.com/sipec"
    },
    {
      "name": "Wesley Hobbie",
      "url": "https://github.com/wslyhbb"
    }
  ],
  "license": "MIT",
  "description": "A tags input component for Vue 3 with autocompletion, custom validation, templating and much more",
  "homepage": "http://www.vue-tags-input.com",
  "private": false,
  "keywords": [
    "javascript",
    "vue",
    "vue3",
    "tags",
    "vue-tags-input",
    "vue-component",
    "autocomplete"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wslyhbb/vue-tags-input.git"
  },
  "main": "dist/vue-tags-input.js",
  "scripts": {
    "dev": "npm run docs",
    "docs": "cross-env NODE_ENV=development webpack serve --hot --config ./build/docs.config.js",
    "build-docs": "cross-env NODE_ENV=production webpack --config ./build/docs.config.js",
    "analyze": "cross-env ANALYZE=true npm run build-docs",
    "build-lib": "webpack --config ./build/lib.config.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --ignore-pattern *.demo.*",
    "lint-fix": "eslint --fix --ignore-path .gitignore --ext .js,.vue . --ignore-pattern *.demo.*",
    "e2e": "testcafe all e2e/*test* --app \"npm run dev\" --app-init-delay 10000 -S -s screenshots"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.vue": "eslint"
  },
  "files": [
    "dist/*",
    "vue-tags-input/*"
  ],
  "peerDependencies": {
    "vue": "3.x"
  },
  "dependencies": {
    "vue": "^3.5.8"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@vue/compiler-sfc": "^3.5.8",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "documentation": "^14.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^8.7.1",
    "fast-deep-equal": "^3.1.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "http-server": "^14.1.1",
    "husky": "^9.1.6",
    "image-webpack-loader": "^8.1.0",
    "ip": "^2.0.1",
    "lint-staged": "^15.2.0",
    "normalize-css": "^2.3.1",
    "postcss": "^8.4.47",
    "postcss-loader": "^4.2.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.79.3",
    "sass-loader": "^10.1.1",
    "testcafe": "^3.0.0",
    "testcafe-vue-selectors": "^3.0.0",
    "url-loader": "^4.1.1",
    "vue-cli-plugin-vue-next": "~0.1.4",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.0.6",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.28.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.1"
  },
  "eslintConfig": {
    "root": true,
    "extends": "plugin:vue/vue3-recommended",
    "rules": {
      "vue/max-attributes-per-line": "off",
      "vue/no-use-v-if-with-v-for": "off",
      "vue/no-unused-components": "off",
      "vue/component-name-in-template-casing": "off",
      "vue/singleline-html-element-content-newline": "off",
      "vue/no-v-html": "off",
      "vue/require-default-prop": "off",
      "vue/use-v-on-exact": "off",
      "vue/multiline-html-element-content-newline": "off"
    }
  },
  "browserslist": [
    "ie >= 11",
    "last 2 Firefox versions",
    "last 5 Chrome versions",
    "last 2 iOS versions"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
